#
#  BUNDLE 
#


Bundle-Version: 1.0.0.${tstamp}


Private-Package: \
	com.icteam.loyalty.common.provider,\
	com.icteam.loyalty.common.provider.tracker
	
-buildpath: \
	osgi.enroute.base.api;version=latest,\
	org.apache.commons.lang3;version=3.4,\
	org.eclipse.equinox.common;version=3.8,\
	com.icteam.loyalty.common.api;version=latest,\
	org.eclipse.jetty.util;version=9.3
	
-includeresource: {readme.md},\
                  OSGI-INF/configuration=resources/configuration

-runrequires: osgi.identity;filter:='(osgi.identity=com.icteam.loyalty.common.provider)'
	
-runbundles: \
	${error;Resolve first}
Bundle-Name: Loyalty Common Provider
Bundle-Activator: com.icteam.loyalty.common.provider.Activator
Enum-Extension: com.icteam.loyalty.common.api.enums.Alfa, com.icteam.loyalty.common.api.enums.Beta
Tracker-Extension: com.icteam.loyalty.common.provider.tracker.EnumTracker,\
                   com.icteam.loyalty.common.provider.tracker.ConfigTracker 	
	
Export-Package: \
	com.icteam.loyalty.common.api,\
	com.icteam.loyalty.common.api.enums,\
	com.icteam.loyalty.common.api.interfaces,\
	com.icteam.loyalty.common.api.nls